super + w
  LIBVA_DRI3_DISABLE=1 LIBVA_DRIVER_NAME=iHD nixGLIntel brave

super + a
  dr_launcher

super + shift + q
  dr_kill

super + shift + e
  dr_emoji

super + KP_Divide
  rofi -show calc -modi calc -no-show-match -no-sort

super + Escape
  dr_power

ctrl + grave
  dunstctl history-pop

super + shift + g
  picom-toggle

super + ctrl + l
  i3exit lock

{_, super +, shift +, ctrl +} @Print
  screenshot {fullscreen, menu, selection, window}

{XF86AudioPrev, XF86AudioPlay, XF86AudioNext}
  playerctl {--player=spotify previous, play-pause, --player=spotify next}

{XF86MonBrightnessUp, XF86MonBrightnessDown}
  pkill -SIGRTMIN+9 i3blocks

{XF86AudioRaiseVolume, XF86AudioLowerVolume, XF86AudioMute, ctrl + XF86AudioMute}
  volume_control {up, down, mute, micmute}; kill -45 $(pidof dwmblocks) && pkill -SIGRTMIN+10 i3blocks

